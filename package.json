{
  "name": "cps-shared-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production --project=cps-ui-kit",
    "watch": "ng build cps-ui-kit --watch --configuration development",
    "test": "npm run cypress:run",
    "lint": "eslint \"**/*.ts\"",
    "cypress:open": "cypress open --component --browser chrome",
    "cypress:run": "cypress run --component",
    "generate-json-api": "node ./api-generator/api-generator.js",
    "dev:ssr": "ng run composition:serve-ssr",
    "serve:ssr": "node dist/composition/server/main.js",
    "build:ssr": "ng build && ng run composition:server",
    "prerender": "ng run composition:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/ssr": "^17.1.0",
    "@types/lodash-es": "^4.17.7",
    "express": "^4.18.2",
    "lodash-es": "^4.17.21",
    "primeng": "^17.3.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/cli": "~17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@cypress/angular": "^2.0.3",
    "@cypress/schematic": "^2.5.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "browser-sync": "^3.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^17.1.0",
    "prettier": "^2.8.8",
    "typedoc": "^0.25.7",
    "typescript": "~5.2.0"
  },
  "optionalDependencies": {
    "cypress": "^12.14.0"
  }
}