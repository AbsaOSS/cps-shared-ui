                    {
                        "name": "disabled",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Determines whether the component is disabled."
                    },
                    {
                        "name": "defaultTime",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "00:00:00",
