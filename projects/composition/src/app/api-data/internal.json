{
    "components": {
        "CpsBaseTreeDropdownComponent": {
            "description": "BaseTreeDropdownComponent is an internal base component to support hierarchical data dropdown.",
            "props": {
                "description": "Defines the input properties of the component.",
                "values": [
                    {
                        "name": "label",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "",
                        "description": "Label of the component."
                    },
                    {
                        "name": "hint",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "",
                        "description": "Bottom hint text."
                    },
                    {
                        "name": "multiple",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Specifies if multiple values can be selected."
                    },
                    {
                        "name": "disabled",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Determines whether the component is disabled."
                    },
                    {
                        "name": "width",
                        "optional": false,
                        "readonly": false,
                        "type": "string | number",
                        "default": "100%",
                        "description": "Width of the component, of type number denoting pixels or string."
                    },
                    {
                        "name": "chips",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "true",
                        "description": "When selecting an element, it will appear in a form of a chip."
                    },
                    {
                        "name": "closableChips",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "true",
                        "description": "Determines whether the chips can be directly removed."
                    },
                    {
                        "name": "clearable",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "When enabled, a clear icon is displayed to clear the value."
                    },
                    {
                        "name": "openOnClear",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "true",
                        "description": "Determines whether the dropdown should open on clear."
                    },
                    {
                        "name": "optionLabel",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "label",
                        "description": "Name of the label field of an option."
                    },
                    {
                        "name": "optionInfo",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "info",
                        "description": "Name of the info field of an option, shows the additional information text."
                    },
                    {
                        "name": "hideDetails",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Options for hiding details."
                    },
                    {
                        "name": "persistentClear",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Determines whether the component should have persistent clear icon."
                    },
                    {
                        "name": "prefixIcon",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "",
                        "description": "Icon before input value."
                    },
                    {
                        "name": "prefixIconSize",
                        "optional": false,
                        "readonly": false,
                        "type": "iconSizeType",
                        "default": "18px",
                        "description": "Size of icon before input value, of type number, string, 'fill', 'xsmall', 'small', 'normal' or 'large'."
                    },
                    {
                        "name": "loading",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "When enabled, a loading bar is displayed."
                    },
                    {
                        "name": "virtualScroll",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Determines whether only the elements within scrollable area should be added into the DOM."
                    },
                    {
                        "name": "numToleratedItems",
                        "optional": false,
                        "readonly": false,
                        "type": "number",
                        "default": "10",
                        "description": "Determines how many additional elements to add to the DOM outside of the view."
                    },
                    {
                        "name": "infoTooltip",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "",
                        "description": "When it is not an empty string, an info icon is displayed to show text for more info."
                    },
                    {
                        "name": "infoTooltipClass",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "cps-tooltip-content",
                        "description": "Info tooltip class for styling."
                    },
                    {
                        "name": "infoTooltipMaxWidth",
                        "optional": false,
                        "readonly": false,
                        "type": "string | number",
                        "default": "100%",
                        "description": "Max width of infoTooltip of type number denoting pixels or string."
                    },
                    {
                        "name": "infoTooltipPersistent",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Determines whether the infoTooltip is persistent."
                    },
                    {
                        "name": "infoTooltipPosition",
                        "optional": false,
                        "readonly": false,
                        "type": "CpsTooltipPosition",
                        "default": "top",
                        "description": "Position of infoTooltip, it can be 'top', 'bottom', 'left' or 'right'."
                    },
                    {
                        "name": "initialExpandDirectories",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "When set, it expands all directiories initially."
                    },
                    {
                        "name": "initialExpandAll",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "When set, it expands all options initially."
                    },
                    {
                        "name": "options",
                        "optional": false,
                        "readonly": false,
                        "type": "TreeNode<any>[]"
                    },
                    {
                        "name": "_value",
                        "optional": false,
                        "readonly": false,
                        "type": "any",
                        "default": "undefined",
                        "description": "Value of the component."
                    }
                ]
            },
            "emits": {
                "description": "Defines emit that determine the behavior of the component based on a given condition or report the actions that the component takes.",
                "values": [
                    {
                        "name": "valueChanged",
                        "parameters": [
                            {
                                "name": "value",
                                "type": "any"
                            }
                        ],
                        "description": "Callback to invoke on value change."
                    }
                ]
            }
        }
    }
}