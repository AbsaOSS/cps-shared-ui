                "values": [
                    {
                        "name": "FORCED",
                        "value": "forced"
                    },
                    {
                        "name": "TOGGLE",
                        "value": "toggle"
                    },
                    {
                        "name": "CLICK_ITEM",
                        "value": "click-item"
                    },
                    {
                        "name": "CLICK_OUTSIDE",
                        "value": "click-outside"
                    },
                    {
                        "name": "KEYDOWN_ESCAPE",
                        "value": "keydown-escape"
                    },
                    {
                        "name": "SCROLL",
                        "value": "scroll"
                    },
                    {
                        "name": "RESIZE",
                        "value": "resize"
                    },
                    {
                        "name": "TARGET_NOT_CONNECTED",
                        "value": "target-not-connected"
                    }
                ]
            }
        ]
    }
}