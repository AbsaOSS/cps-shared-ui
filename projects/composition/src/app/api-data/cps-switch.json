{
    "components": {
        "CpsSwitchComponent": {
            "description": "CpsSwitchComponent is a component used to toggle a boolean value.",
            "props": {
                "description": "Defines the input properties of the component.",
                "values": [
                    {
                        "name": "label",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "",
                        "description": "Label of the component."
                    },
                    {
                        "name": "disabled",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Determines whether the component is disabled."
                    },
                    {
                        "name": "infoTooltip",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "",
                        "description": "When it is not an empty string, an info icon is displayed to show text for more info."
                    },
                    {
                        "name": "infoTooltipClass",
                        "optional": false,
                        "readonly": false,
                        "type": "string",
                        "default": "cps-tooltip-content",
                        "description": "InfoTooltip class for styling."
                    },
                    {
                        "name": "infoTooltipMaxWidth",
                        "optional": false,
                        "readonly": false,
                        "type": "string | number",
                        "default": "100%",
                        "description": "Size of infoTooltip, of type number denoting pixels or string."
                    },
                    {
                        "name": "infoTooltipPersistent",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Determines whether the infoTooltip is persistent."
                    },
                    {
                        "name": "infoTooltipPosition",
                        "optional": false,
                        "readonly": false,
                        "type": "CpsTooltipPosition",
                        "default": "top",
                        "description": "Position of infoTooltip, it can be 'top', 'bottom', 'left' or 'right'."
                    },
                    {
                        "name": "value",
                        "optional": false,
                        "readonly": false,
                        "type": "boolean",
                        "default": "false",
                        "description": "Value of the switch component."
                    }
                ]
            },
            "emits": {
                "description": "Defines emit that determine the behavior of the component based on a given condition or report the actions that the component takes.",
                "values": [
                    {
                        "name": "valueChanged",
                        "parameters": [
                            {
                                "name": "value",
                                "type": "boolean"
                            }
                        ],
                        "description": "Callback to invoke on value change."
                    }
                ]
            }
        }
    }
}